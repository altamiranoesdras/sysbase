{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "acacha/admin-lte-template-laravel": "^6.0",
        "barryvdh/laravel-snappy": "^0.4.0",
        "codedge/laravel-fpdf": "^1.2",
        "doctrine/dbal": "~2.3",
        "dompdf/dompdf": "^2.0.0",
        "fideloper/proxy": "~4.0",
        "infyomlabs/adminlte-templates": "5.7.x-dev",
        "infyomlabs/laravel-generator": "5.7.x-dev",
        "infyomlabs/swagger-generator": "dev-master",
        "jenssegers/date": "^3.4",
        "jlapp/swaggervel": "dev-master",
        "laravel/framework": "5.7.*",
        "laravel/passport": "^7.1",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.7.0",
        "laraveles/spanish": "^1.1",
        "owen-it/laravel-auditing": "^4.0",
        "spatie/laravel-permission": "^2.28",
        "yajra/laravel-datatables-buttons": "^4.4",
        "yajra/laravel-datatables-oracle": "~8.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "barryvdh/laravel-ide-helper": "^2.4",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "orangehill/iseed": "dev-master",
        "phpunit/phpunit": "^7.0",
        "xethron/migrations-generator": "dev-master"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
